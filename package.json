{
    "name": "@priorit/localiser",
    "version": "1.0.0",
    "description": "Utility functions to fix localisation issues in TypeScript",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "src",
        "dist"
    ],
    "scripts": {
        "prepack": "tsc -p tsconfig.json",
        "test": "jasmine --config=jasmine.json",
