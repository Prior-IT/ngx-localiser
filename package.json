        "codelint": "eslint -c .eslintrc.json src/*",
        "stylelint": "npx stylelint '**/*.scss'"
    },
    "repository": {
        "type": "git",
        "url": "git+https://gitlab.com/priorit/piworks/pilocaliser-ts.git"
    },
